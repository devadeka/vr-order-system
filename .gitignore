###############################################################################################
# Ignore specific config files
###############################################################################################
/vr-order-backend-rails/.bundle/*
!/vr-order-backend-rails/.bundle/config
/vr-order-backend-rails/config/application.yml


###############################################################################################
# Ignore all logfiles and tempfiles.
###############################################################################################
/vr-order-backend-rails/log/*.log
/vr-order-backend-rails/tmp



###############################################################################################
# Ignore local asset pipline cache files, local Paperclip Carrierwave folders
###############################################################################################
/vr-order-backend-rails/public/assets
/vr-order-backend-rails/public/system/*


###############################################################################################
# Packages #
###############################################################################################
*.7z
*.dmg
*.gz
*.iso
*.jar
*.rar
*.tar
*.zip


###############################################################################################
# Logs and databases #
###############################################################################################
*.log
*.sql
*.sqlite


###############################################################################################
# OS generated files #
###############################################################################################
.DS_Store
.DS_Store?
._*
.Spotlight-V100
.Trashes
Icon?
ehthumbs.db
Thumbs.db
